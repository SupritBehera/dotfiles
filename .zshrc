# Skim Search (cd or open up a file)
skim()
{
    RESULT_DIR=$(sk)
    if [ -f $RESULT_DIR ]
    then
        xdg-open $RESULT_DIR
    elif [ -d $RESULT_DIR ]
    then
        cd $RESULT_DIR
    fi
}

